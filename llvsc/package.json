{
	"name": "llvsc",
	"displayName": "llvsc",
	"description": "llvsc",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:cpp",
		"onLanguage:c"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "llvsc.smartSpace",
				"title": "Smart Space",
				"category": "llvsc"
			},
			{
				"command": "llvsc.smartSemicolon",
				"title": "Smart Semicolon",
				"category": "llvsc"
			},
			{
				"command": "llvsc.smartColon",
				"title": "Smart Colon",
				"category": "llvsc"
			},
			{
				"command": "llvsc.smartEnter",
				"title": "Smart Enter",
				"category": "llvsc"
			}
		],
		"keybindings": [
			{
				"command": "llvsc.smartSpace",
				"key": "space",
				"when": "editorTextFocus"
			},
			{
				"command": "llvsc.smartSemicolon",
				"key": ";",
				"when": "editorTextFocus"
			},
			{
				"command": "llvsc.smartColon",
				"key": "shift+;",
				"when": "editorTextFocus"
			},
			{
				"command": "llvsc.smartEnter",
				"key": "shift+enter",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"vscode-debugadapter": "^1.35.0",
		"vscode-debugprotocol": "^1.35.0",
		"vscode-extension-telemetry": "^0.1.2"
	}
}
